<Window x:Class="UIT_TimeTable.WindowView.GroupBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UIT_TimeTable.WindowView"
        mc:Ignorable="d"
        Title="About Middleshire Bell Ringers" Height="160" Width="300" 
        ResizeMode="NoResize"
        AllowsTransparency="True"
        WindowStyle="None">
    <Window.Background>
        <SolidColorBrush Opacity="0.97" Color="Gray"/>
    </Window.Background>
    <Grid>
        <Label Content="Version 1.0" Height="28" HorizontalAlignment="Left" Margin="30,20,0,0" Name="version" VerticalAlignment="Top" Width="75" />
        
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="100,85,0,0" Name="ok" VerticalAlignment="Top" Width="78" Click="ok_Click"/>
    </Grid>
</Window>
